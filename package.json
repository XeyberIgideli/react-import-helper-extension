{
  "name": "react-import-helper",
  "displayName": "React Import Helper",
  "description": "This extension automatically creates a folder and file when a React component import is detected.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [ "onStartupFinished" ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.importHelper",
        "title": "Run React Import Helper"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.54.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "resolve": "^1.22.8"
  }
}
